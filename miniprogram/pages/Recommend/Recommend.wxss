.wrap{
    font-size: 20px;
    font-weight: bold;
    background:-moz-linear-gradient(top,#fff,#65a1ee);
    background:-webkit-gradient(linear,60% 50%,0 20%,from(#65a1ee),to(#fff));
    background:-webkit-linear-gradient(bottom,#fff,#65a1ee);
    background:-o-linear-gradient(bottom,#fff,#65a1ee);
}

.menu{
    position: absolute;
    top:1px;
    left:1px;
    background: url(../AI/AI) no-repeat center center;
    border-radius: 5px;
    font-size: 13px;
    height: 10px;
    width:50px;
    line-height: 10px;
    color: #fff;
    text-align: center;
}

.textBox{
    margin: 0px;
}

.btnBox{
    margin:20px 0;
}

/* New styles for logo */
.logo {
    display: flex;
    justify-content: center;
    margin: 20px 0;
}

/* Adjusted button style to have white text on a blue background */
.whiteOnBlue {
    color: #FFFFFF;
    background-color: #47c0fd;
    border-color: #1c7eff;
}

/* Ensure button inherits these styles without additional styling from the framework */
.whiteOnBlue:hover, .whiteOnBlue:active {
    background-color: #1c7eff;
    border-color: #1c7eff;
}

.item{
    height: 100px;
    line-height: 200px;
    text-align: center;
}
